{
  "name": "bottroner",
  "version": "1.0.0",
  "description": "Discord bot with modular architecture for public use",
  "module": "src",
  "type": "module",
  "private": true,
  "license": "AGPL-3.0",
  "author": "Troner14",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troner14/BotTroner.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "bun",
    "prisma"
  ],
  "scripts": {
    "script:folder": "bun run ./scripts/generateListFolderType.ts",
    "script:translation": "bun run ./scripts/generateTranslationTypes.ts",
    "start": "bun run .",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "update:db": "prisma db push"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "pino-pretty": "^13.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "pino": "^10.1.0",
    "prisma": "^6.17.1",
    "winston": "^3.18.3"
  }
}
