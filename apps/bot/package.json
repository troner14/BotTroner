{
    "name": "@bot/bot",
    "description": "Discord bot with modular architecture",
    "authors": "Troner14",
    "version": "1.0.0",
    "main": "src/index.ts",
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "script:folder": "bun run ./scripts/generateListFolderType.ts",
        "script:translation": "bun run ./scripts/generateTranslationTypes.ts",
        "script:permissions": "bun run ./scripts/generatePermsIdTypes.ts",
        "dev": "bun run src/index.ts",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "test:coverage": "bun test --coverage",
        "test:unit": "bun test tests/unit",
        "test:integration": "bun test tests/integration",
        "update:db": "prisma db push"
    },
    "dependencies": {
        "@bot/database": "workspace:^",
        "@bot/shared-types": "workspace:^",
        "@bot/logger": "workspace:^",
        "@bot/env": "workspace:^",
        "@prisma/adapter-mariadb": "^7.1.0",
        "@prisma/client": "^7.0.1",
        "discord.js": "^14.23.2",
        "html-minifier-terser": "^7.2.0",
        "prisma": "^7.0.1",
        "winston": "^3.18.3"
    }
}