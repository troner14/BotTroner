{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "DATABASE_URL"
  ],
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "env": ["*"]
    },
    "DB:generate": {
      "outputs": []
    },
    "DB:push": {
      "outputs": []
    }
  }
}